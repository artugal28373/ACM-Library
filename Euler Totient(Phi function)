#include<bits/stdc++.h>
#define N 1000000
using namespace std;
int phi[N+2];
bool vis[N+2];
void eulerTotient()
{
    for(int i=1; i<=N; i++)phi[i]= i;
    for(int i=2; i<=N; i++)
    if(!vis[i])
    {
        phi[i]= i-1;
        for(int j=2*i; j<=N; j+=i)
        vis[j]=1, phi[j]/= i, phi[j]*= (i-1);
    }
    return;
}
